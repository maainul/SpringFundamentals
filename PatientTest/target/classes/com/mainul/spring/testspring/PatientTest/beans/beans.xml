<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="patient"
		class="com.mainul.spring.testspring.PatientTest.Patient"
		scope="prototype">
	<constructor-arg value="5" name="id"></constructor-arg>
	<constructor-arg value="Tom" name="nameString"></constructor-arg>
	<property name="nationalId" value="1234"></property>
	<property name="address" ref="address"></property>
	</bean>

	<bean id="address"
		class="com.mainul.spring.testspring.PatientTest.Address">
	<constructor-arg name="streetString" value="Panthpath"></constructor-arg>
	<constructor-arg name="postcodeString" value="1200"></constructor-arg>
	</bean>
</beans>
